<a 
  {{action "clicked"}}
  class="login-icon-box v-center-parent auth-icon {{authIdClass}} {{if (eq authId "onepanel") "username"}}"
  style={{aStyle}}
>
  {{#if active}}
    <div class="v-center-child" style={{spinnerBoxStyle}}>
      {{spin-spinner scale=spinnerScale lines=12 length=20 width=10}}
    </div>
  {{else}}
    <div class="social-icon-image" style={{socialIconStyle}}></div>
  {{/if}}
</a>
{{#if tip}}
  {{bs-tooltip title=tip}}
{{/if}}

