{{#onezone-atlas deselectProviders='deselectProviders' as |atlas|}}
  {{#each model as |provider|}}
    {{provider-place provider=provider atlas=atlas selectProvider='selectProvider'}}
  {{/each}}

  {{#if isLoading}}
    <div class="spinner-container spinner-centered spinner-middle">
      {{spin-spinner scale=2 lines=12 length=20 width=10}}
    </div>
  {{else}}
    {{onezone-modal-container providers=model}}
  {{/if}}
{{/onezone-atlas}}
