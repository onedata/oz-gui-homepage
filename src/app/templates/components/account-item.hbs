<div class="account-icon icon secondary-item-element">
  <div class="circle {{type}}"></div>
  <div class="icon">
    {{#if (eq iconType "oneicon")}}
      {{one-icon tagName="div" icon=iconName}}
    {{else}}
      <div class="social-icon-image" style={{socialIconStyle}}></div>
    {{/if}}
  </div>
</div>
{{#truncated-string class="account-label one-label one-label-large secondary-item-element"}}
  {{label}}
{{/truncated-string}}
